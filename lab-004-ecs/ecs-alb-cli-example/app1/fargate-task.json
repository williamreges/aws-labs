{
  "family": "app1-task",
  "networkMode": "awsvpc",
  "taskRoleArn": "arn:aws:iam::940308622416:role/ecsTaskExecutionRole",
  "executionRoleArn": "arn:aws:iam::940308622416:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "container-app1",
      "image": "940308622416.dkr.ecr.sa-east-1.amazonaws.com/app1",
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp",
          "name": "container-app1-80-tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "environment": [],
      "environmentFiles": [],
      "mountPoints": [],
      "volumesFrom": [],
      "ulimits": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/app1-task",
          "mode": "non-blocking",
          "awslogs-create-group": "true",
          "max-buffer-size": "25m",
          "awslogs-region": "sa-east-1",
          "awslogs-stream-prefix": "ecs"
        },
        "secretOptions": []
      },
      "systemControls": []
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512"
}